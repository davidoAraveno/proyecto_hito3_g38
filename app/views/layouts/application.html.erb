<!DOCTYPE html>
<html>
  <head>
    <title>Novios</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <%= notice %>
    <% if session[:marriage] || session[:invited] || session[:admin] %>
      <%= link_to 'cerrar sesion',s_d_path, method: :delete %>
    <% end %>

    <%= yield %>
  </body>
</html>
